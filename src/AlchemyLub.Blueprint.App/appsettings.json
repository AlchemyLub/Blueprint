{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "PostgreSQL": "Server=<Server>;Port=<Port>;Database=<Database>;User Id=<UserId>;Password=<Password>;Integrated Security=true;Pooling=true;",
    "Redis": "localhost:6379,defaultDatabase=1"
  }
}
